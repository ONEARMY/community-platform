    "firebase-functions": "^3.14.1",
    "firebase-tools": ">=9.16.5",
    "fs-extra": "^9.0.1",
    "google-auth-library": "^6.1.1",
    "googleapis": "^61.0.0",
    "log-update": "^4.0.0",
    "oa-shared": "^1.0.0",
    "one-army-community-platform": "^1",
    "request": "^2.88.2",
    "sharp": "^0.26.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/concurrently": "^6.2.1",
