{
  "name": "oa-components",
  "version": "1.0.0",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "storybook": "yarn start",
    "start": "storybook dev -p 6006 --loglevel verbose",
    "format": "prettier --write '**/*.{ts,tsx,mdx,md}'",
    "build:sb": "yarn build --build && storybook build",
    "build": "tsc --build --force --verbose",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx src --color",
    "new-component": "ts-node scripts/newComponent.ts",
    "test": "vitest",
    "test-ci": "vitest --coverage --reporter=junit --outputFile.junit=./reports/output.xml",
    "chromatic": "npx chromatic -b=build:sb --exit-zero-on-changes"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@faker-js/faker": "^7.6.0",
    "@mdxeditor/editor": "^3.29.2",
    "@mui/base": "next",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "chromatic": "^11.4.0",
    "date-fns": "^2.29.3",
    "framer-motion": "^12.11.1",
    "linkify-plugin-mention": "^4.0.2",
    "linkify-react": "^4.0.2",
    "linkifyjs": "^4.0.2",
    "oa-themes": "workspace:^",
    "photoswipe": "^5.4.1",
    "react-country-flag": "^3.1.0",
    "react-horizontal-scrolling-menu": "^8.2.0",
    "react-icons": "^5.3.0",
    "react-image-crop": "^11.0.5",
    "react-markdown": "^10.1.0",
    "react-player": "3.0.0-canary.4",
    "react-portal": "^4.2.2",
    "react-responsive-masonry": "2.1.7",
    "react-router": "6.24.1",
    "react-router-dom": "^6.26.0",
    "react-select": "^5.8.1",
    "react-tooltip": "^5.28.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-youtube": "^1.3.2",
    "storybook": "^7.6.0",
    "styled-system": "^5.1.5",
    "theme-ui": "^0.16.2",
    "use-debounce": "^8.0.4",
    "webpack": "5.76.0",
    "yup": "^1.3.2"
  },
  "peerDependencies": {
    "react": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@remix-run/dev": "^2.12.1",
    "@remix-run/testing": "^2.12.1",
    "@storybook/addon-actions": "^7.4.1",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/addon-mdx-gfm": "^7.4.1",
    "@storybook/addons": "^7.4.1",
    "@storybook/react": "^7.4.1",
    "@storybook/react-vite": "^7.4.1",
    "@storybook/theming": "^7.4.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "12",
    "@types/mustache": "^4.1.2",
    "@types/react": "^18.3.3",
    "@types/react-flag-icon-css": "^1.0.5",
    "@types/react-portal": "^4.0.4",
    "@types/react-responsive-masonry": "^2.1.3",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "babel-loader": "8.1.0",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^21.1.1",
    "mustache": "^4.2.0",
    "prettier": "2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^5.1.6",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.6.0"
  }
}
