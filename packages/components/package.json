{
  "name": "oa-components",
  "version": "1.0.0",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.js",
  "scripts": {
    "storybook": "yarn start",
    "start": "storybook dev -p 6006 --loglevel verbose",
    "format": "prettier --write '**/*.{ts,tsx,mdx,md}'",
    "build:sb": "yarn build --build && storybook build",
    "build": "tsc --build --verbose",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx src --color",
    "test": "vitest",
    "test-ci": "vitest --coverage --reporter=junit --outputFile.junit=./reports/output.xml"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
